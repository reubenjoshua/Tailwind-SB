 },'js-esKuZQChgfekjbQThvda': function (cdx) { var getDLV = key => {
  var dl = window.dataLayer;
  if (typeof dl !== 'undefined') {
    for (var i = 0; i < dl.length; i++) {
      if (typeof dl[i][key] !== 'undefined') {
        return dl[i][key];
      }
    }
  }
  return null;
};

var isTrialUser = getDLV('isTrialUser');
var isRecurring = getDLV('isRecurring');  
var currentPlan = getDLV('currentPlan'); 

if (isTrialUser === false && isRecurring === true && currentPlan === 'premium') {
  return true
}

